<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/_cbcbcb"></solid>
            <corners
                android:bottomLeftRadius="@dimen/_15px"
                android:bottomRightRadius="@dimen/_15px"
                android:topLeftRadius="@dimen/_15px"
                android:topRightRadius="@dimen/_15px">
            </corners>

        </shape>
    </item>
    <item android:top="1px" android:bottom="1px" android:left="1px" android:right="1px">
        <shape android:shape="rectangle">
            <solid android:color="@color/_ffffff"></solid>
            <corners
                android:bottomLeftRadius="@dimen/_15px"
                android:bottomRightRadius="@dimen/_15px"
                android:topLeftRadius="@dimen/_15px"
                android:topRightRadius="@dimen/_15px">
            </corners>
        </shape>
    </item>

</layer-list>