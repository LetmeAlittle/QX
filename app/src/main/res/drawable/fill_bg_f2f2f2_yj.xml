<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/_f2f2f2"></solid>
            <corners
                android:bottomLeftRadius="@dimen/_30px"
                android:bottomRightRadius="@dimen/_30px"
                android:topLeftRadius="@dimen/_30px"
                android:topRightRadius="@dimen/_30px">
            </corners>

        </shape>
    </item>
    <item android:top="2px" android:bottom="2px" android:left="2px" android:right="2px">
        <shape android:shape="rectangle">
            <solid android:color="@color/_f2f2f2"></solid>
            <corners
                android:bottomLeftRadius="@dimen/_30px"
                android:bottomRightRadius="@dimen/_30px"
                android:topLeftRadius="@dimen/_30px"
                android:topRightRadius="@dimen/_30px">
            </corners>
        </shape>
    </item>

</layer-list>